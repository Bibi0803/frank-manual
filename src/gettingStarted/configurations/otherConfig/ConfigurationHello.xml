<Module
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="./ibisdoc.xsd">
  <Adapter name="otherConfigHello">
    <Receiver name="otherConfigHelloReceiver">
      <JavaListener name="otherConfigHelloListener" />
    </Receiver>
    <Pipeline firstPipe="otherConfigHelloPipe">
      <Exit path="EXIT" state="success"/>
      <FixedResultPipe name="otherConfigHelloPipe" returnString="Hello from other config"/>
    </Pipeline>
  </Adapter>
</Module>