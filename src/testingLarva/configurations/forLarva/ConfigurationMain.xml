<Module
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="./ibisdoc.xsd">
  <Adapter name="adapterMain">
    <Receiver name="receiverMain">
      <JavaListener name="listenerMain" serviceName="listenerMain"/>
    </Receiver>
    <Pipeline firstPipe="pipeResult">
      <SenderPipe name="pipeResult">
        <IbisJavaSender
          name="callDelegate"
          serviceName="listenerDelegate">
        </IbisJavaSender>
        <Forward name="success" path="Exit"/>
      </SenderPipe>
      <Exit path="Exit" state="success" code="200"/>
    </Pipeline>
  </Adapter>
</Module>